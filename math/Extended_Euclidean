ll Ex_Euclead(ll a, ll b = mod) {
    ll q;
    vector<ll> line1 = {a, 1, 0};
    vector<ll> line2 = {b, 0, 1};
    vector<ll> line3 = {0, 0, 0};
    while (line2[0] > 0) {
        q = line1[0]/ line2[0];
        for(int i = 0; i < 3; i++) {
            line3[i] = (line1[i] - q* line2[i])%mod;
        }
        line1 = line2;
        line2 = line3;
    }
    return line1[1];
}
